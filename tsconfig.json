{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "strict": false,
    "moduleResolution": "node",
    "types": ["react", "react-native"],
    "resolveJsonModule": true
  },
  "exclude": ["node_modules"]
}
